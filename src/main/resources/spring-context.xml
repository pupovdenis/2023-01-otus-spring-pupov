<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="quizPrinter" class="ru.pupov.service.impl.QuizPrinterImpl"/>
    <bean id="questionExtractor" class="ru.pupov.service.impl.QuestionExtractorImpl"/>

    <bean id="quizService" class="ru.pupov.service.impl.QuizServiceImpl">
        <constructor-arg name="quizPrinter" ref="quizPrinter"/>
        <constructor-arg name="questionExtractor" ref="questionExtractor"/>
        <constructor-arg name="csvPath" value="data.csv" type="java.lang.String"/>
    </bean>
</beans>